{
  "version": 2,
  "routes": [
    {
      "src": "^/service-worker.js$",
      "dest": "/_next/static/service-worker.js",
      "headers": {
        "cache-control": "public, max-age=43200, immutable",
        "Service-Worker-Allowed": "/"
      }
    }
  ],
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    }
  ],
  "env": {
    "API_URL": "@devgulp-api-url",
    "DEVGULP_STRIPE_PUBLIC_KEY_TEST": "@devgulp-stripe-public-key-test",
    "DEVGULP_STRIPE_SECRET_KEY_TEST": "@devgulp-stripe-secret-key-test"
  }
}
